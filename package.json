{
  "name": "react-power-picture",
  "version": "0.0.0-development",
  "description":
    "A React component for rendering images both resposively and progressively",
  "keywords": [
    "react",
    "responsive",
    "progressive",
    "images",
    "picture",
    "loading",
    "performance",
    "render-props"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
